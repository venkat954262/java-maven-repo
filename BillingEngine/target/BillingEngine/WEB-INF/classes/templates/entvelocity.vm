<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>APSFL Bill Document</title>
<style>
body { background-image:url(logo(1).png); background-repeat:repeat; 
    margin: 0;
    padding: 0;
    background-color: #FAFAFA;
    font: 11px "Arial";
}
* {
    box-sizing: border-box;
    -moz-box-sizing: border-box;
}
.page {
    width: 21cm;
    min-height: 29.7cm;
    padding: 1cm;
    margin: 1cm auto;
    border: 1px #fffff solid;
    border-radius: 1px;
    background: white;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
}
.subpage {
    padding: 0.5cm;
    border: 2px #E7E7E7 solid;
    height: 256mm;
    outline:1cm #ffffff solid;
}

@page {
    size: A4;
    margin: 0;
}
@media print {
    .page {
        margin: 0;
        border: initial;
        border-radius: initial;
        width: initial;
        min-height: initial;
        box-shadow: initial;
        background: initial;
        page-break-after: always;
    }
}
.mainbody {
    height:200mm;
    width:15.cm;
    
}

label {
	font-weight: bold;
	font-size: 13px;
}
.intext{ padding-left:5px;  border-left:solid; border-left-width:1px;}
.showtext{ padding-right:5px; text-align:right; border-left:solid; border-left-width:1px;}
.midtext{ padding:5px; text-align:center;}
.header {
    height: 90px;
    padding: 0px;
}
table
{
   border-collapse:collapse;
}
tr
{
   border-bottom: 1px solid #000;
}
tr:last-child
{
   border-bottom: none;
}

#macro( processSimpleData $data )
 #if ($StringUtils.isNotEmpty($data))
      $data
    #else
      
    #end
#end
#macro( processData $data )
